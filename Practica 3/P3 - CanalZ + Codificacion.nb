Cell[BoxData[
 RowBox[{
  RowBox[{"Pe", "[", 
   RowBox[{"n_", ",", "p_"}], "]"}], ":=", 
  RowBox[{
   UnderoverscriptBox["\[Sum]", 
    RowBox[{"k", "=", 
     RowBox[{"n", "+", "1"}]}], 
    RowBox[{
     RowBox[{"2", "*", "n"}], "+", "1"}]], 
   RowBox[{
    RowBox[{"Binomial", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"2", "n"}], "+", "1"}], ",", "k"}], "]"}], "*", 
    RowBox[{"p", "^", "k"}], " ", "*", " ", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", "p"}], ")"}], "^", 
